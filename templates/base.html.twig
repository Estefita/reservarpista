        {% block javascripts %}
            <script>
                $(document).ready(function () {
                    $("#auComplete").autocomplete({
                        source: function (request, response) {                    
                            $.ajax({
                                url: "/busqueda",                       
                                method:"POST",
                                data: {
                                    texto: $('#auComplete').val()
                                }
                            })                   
                            .done(function( result ) {
                            response($.map(result.texto, function(item){
                                // console.log(item);
                                    return {label: item.textobusqueda, value: item.textobusqueda, admin1code: item.admin1code}
                            }));    
                            });
                        },
                        minLength: 3,
                        select: function (event, ui) {
                            console.log(ui.item);
                            console.log("Selected: " + ui.item.value + " aka " + ui.item.admin1code);
                        }   
                    });
                });
            </script>
         {% endblock %}
    </body>
</html>
